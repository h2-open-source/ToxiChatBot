{
  "name": "toxichatbot",
  "version": "0.0.1",
  "description": "A Telegram group management bot",
  "main": "index",
  "module": "commonjs",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "googleapis": "^110.0.0",
    "grammy": "^1.3.3",
    "localenv": "^0.2.2",
    "mongoose": "^6.0.5",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@grammyjs/types": "^2.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start:local": "nodemon --watch \"src/**\" --ext \"ts,js,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/devStart.ts\"",
    "start:docker": "docker-compose up -d",
    "lint": "yarn run eslint src",
    "build": "tsc",
    "serve": "ts-node ./src/index.ts",
    "test": "jest --verbose",
    "test:docker": "docker-compose exec node sh -c \"./node_modules/.bin/jest --verbose\""
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@notabug.org:jimmyh/ToxiChatBot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "javascript",
    "node"
  ],
  "author": "Jimmy Hogoboom",
  "license": "AGPL-3.0-or-later"
}
